install.packages("ggplot2")
install.packages("dplyr")
install.packages("e1071")
install.packages("nortest")
library(ggplot2)
library(dplyr)
library(e1071)
library(nortest)
df <- read.csv("ElectricCarData.csv")
attribute <- 'AccelSec'
ggplot(df, aes(x = .data[[attribute]])) +
geom_histogram(aes(y = ..density..), bins = 20, fill = 'lightblue', color = 'black') +
geom_density(color = 'red', size = 1) +
ggtitle(paste('Histogram and Density Line of', attribute)) +
xlab(attribute) +
ylab('Density')
